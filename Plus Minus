void plusMinus(vector<int> arr) {
    int n=arr.size();
 int pos=0,neg=0,zero=0;
    for(int i=0;i<n;i++){
        if(arr[i]>0){
            pos++;
        }
        else if(arr[i]<0){
            neg++;
        }
        else{
              zero++;
        }
    }
    cout << setprecision(6) << (float) pos/ n << endl;
            cout << setprecision(6) << (float) neg/ n << endl;
            cout << setprecision(6) << (float) zero / n << endl;
}
