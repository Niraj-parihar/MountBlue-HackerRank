int divisibleSumPairs(int n, int k, vector<int> ar) {
       int count=0;
           for(int i=0;i<n-1;i++){
for(int j=i+1;j<n;j++){
    if( (ar[i]+ar[j])%k ==0){
count++;
} 
}}

    while(i<n-1 && j<n){
        if( (ar[i]+ar[j])%k ==0){
count++;
}
             else  i++; j++;
              }
    
    return count;
    
}
