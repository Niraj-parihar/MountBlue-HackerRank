string superReducedString(string s) {
    if (s.size() >= 2) {
        for (int i = 1; i < s.size(); i++) {
            if (s[i] == s[i - 1]) {
                s = s.erase(i - 1, 2);
                if (s.empty()) return "Empty String";
                i = 0;
            }
        }
    }
    return s;
}
